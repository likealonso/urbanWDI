<div class="container-signup" style="background-color: #E1E1E3">
  <div class="login-page">
    <div class="word-name">Log In</div>
  
      <p id="alert"><%= alert %></p>
      
      <%= form_tag sessions_path do %>
        <form>
          <div class="form-group">
            <%= label_tag :email %>
            <%= text_field_tag :email, params[:email], options={type:"email", class:"form-control"} %>
          </div>
          <br>
          <div class="form-group">
            <%= label_tag :password %>
            <%= password_field_tag :password, nil, {type:"password", class:"form-control"} %>
          </div>
          <br>
          <div class="form-group">
            <%= submit_tag "Log In", options={class:"btn btn-primary ", 
            style:"background-color: #515151; border: 1px solid #515151"} %>
          </div>
        </form>
      <% end %>
    <div>
  </div>


